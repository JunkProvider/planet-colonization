<UserControl x:Class="SpaceLogistic.WpfView.View.Icons.OkIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             mc:Ignorable="d"
             IsHitTestVisible="False" >
    <Border x:Name="Border" BorderBrush="ForestGreen" IsHitTestVisible="False">
        <Path x:Name="Path"
              HorizontalAlignment="Center" 
              VerticalAlignment="Center"
              Stroke="ForestGreen"
              ClipToBounds="False" 
              IsHitTestVisible="False" />
    </Border>
    <!--<Border x:Name="Border" Width="250" Height="250" BorderBrush="ForestGreen" BorderThickness="10" CornerRadius="99999">
        <Path x:Name="Path"
              HorizontalAlignment="Center" 
              VerticalAlignment="Center" 
              Data="M0,50 L50,100 L150,0"
              StrokeThickness="10"
              Stroke="ForestGreen"
              ClipToBounds="False"  />
    </Border>-->
</UserControl>
